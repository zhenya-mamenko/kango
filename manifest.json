{
  "manifest_version": 3,
  "name": "__MSG_extension_name__",
  "version": "1.3.0",
  "description": "__MSG_extension_description__",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "sidePanel",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "action": {
    "default_title": "__MSG_extension_name__"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en"
}